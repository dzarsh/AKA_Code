def PencarianBiner (bin, L, n):
   if len(L) == 0:
        return false
   else:
        mid = len(L) / 2
        if n == L[mid]:
           return true
        else:
            if n > L[mid]:
                return bin.PencarianBiner(L[mid+1:], n)
            else:
                return bin.PencarianBiner(L[:mid], n)
